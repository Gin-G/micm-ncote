[ccpp-arg-table]
  name = molec_ox_xsect_init
  type = scheme
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = out
  optional = F
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out
  optional = F

########################################################################
[ccpp-arg-table]
  name = molec_ox_xsect_run
  type = scheme
[nlev]
  standard_name = num_levels_for_photolysis
  long_name = number of column layers
  units = count
  dimensions = ()
  type = integer
  intent = in
  optional = F
[tuv_n_wavelen]
  standard_name = tuv_n_actinic_flux_wavelengths
  units = count
  dimensions = ()
  type = integer
  intent = in
[nlevelsMinus1]
  standard_name = num_levels_for_photolysis_minus_1
  long_name = number of column layers minus 1
  units = count
  dimensions = ()
  type = integer
  intent = in
[zen]
  standard_name = solar_zenith
  long_name = solar zenith angle
  units = degrees
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
  optional = F
[alt]
  standard_name = layer_altitude
  long_name = mid-point layer altitude
  units = m
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[temp]
  standard_name = layer_temperature
  long_name = mid-point layer temperature
  units = K
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[press_mid]
  standard_name = layer_pressure
  long_name = mid-point layer pressure
  units = Pa
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[o2vmr]
  standard_name = o2_vmr_col
  long_name = O2 volume mixing ratio column
  units = mole/mole
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[dto2]
  standard_name = o2_optical_depth
  long_name = optical depth due to O2 absorption
  units = cm
  dimensions = (nlevelsMinus1,tuv_n_wavelen)
  type = real
  kind = kind_phys
  intent = out
  optional = F
[srb_o2_xs]
  standard_name = o2_xsect
  long_name = O2 effective cross section
  units = cm2
  dimensions = (tuv_n_wavelen,nlevels)
  type = real
  kind = kind_phys
  intent = out
  optional = F
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = out
  optional = F
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out
  optional = F

